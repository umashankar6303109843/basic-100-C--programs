#include <iostream>
#include <string>
using namespace std;
 
int main () {
  // Creating variables
  int myNum = 5;               
  float FloatNum = 5.99;     
  double DoubleNum = 9.98;   
  char Letter = 'D';         
  bool Boolean = true;        
  string String = "Hello"; 
   
  // Print variable values
  cout << "int: " << Num << "\n";
  cout << "float: " << FloatNum << "\n";
  cout << "double: " << DoubleNum << "\n";
  cout << "char: " << Letter << "\n";
  cout << "bool: " << Boolean << "\n";
  cout << "string: " << String << "\n";
 
  return 0;
}
